{
	"name": "simple-axios-case-converter",
	"version": "1.0.5",
	"author": "yutak23 <yuta.katayama.23+github@gmail.com> (https://github.com/yutak23)",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com:yutak23/simple-axios-case-converter.git"
	},
	"license": "MIT",
	"type": "module",
	"types": "./esm/index.d.ts",
	"main": "./cjs/index.js",
	"module": "./esm/index.js",
	"exports": {
		".": {
			"import": {
				"types": "./esm/index.d.ts",
				"default": "./esm/index.js"
			},
			"require": {
				"types": "./cjs/index.d.ts",
				"default": "./cjs/index.js"
			},
			"default": "./cjs/index.js"
		}
	},
	"scripts": {
		"build": "run-s clean build:*",
		"build:esm": "tsc -p ./tsconfig.esm.json && echo '{\"type\":\"module\"}' > esm/package.json",
		"build:cjs": "tsc -p ./tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > cjs/package.json",
		"clean": "rm -rf ./esm ./cjs",
		"test": "vitest run --coverage",
		"lint": "eslint . --ext .cjs,.mjs,.js,.ts,.cts,.mts",
		"lint:fix": "eslint . --ext .cjs,.mjs,.js,.ts,.cts,.mts --fix",
		"format": "prettier -c .",
		"format:fix": "prettier --write .",
		"ncu": "ncu"
	},
	"dependencies": {
		"axios": "^1.7.9",
		"camelcase-keys": "^9.1.3",
		"snakecase-keys": "^8.0.1"
	},
	"devDependencies": {
		"@tsconfig/node16": "^16.1.3",
		"@tsconfig/recommended": "^1.0.8",
		"@types/node": "^18.19.74",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"@vitest/coverage-v8": "^3.0.5",
		"eslint": "^8.57.1",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^18.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.31.0",
		"npm-check-updates": "^17.1.14",
		"npm-run-all2": "^7.0.0",
		"prettier": "^3.4.2",
		"typescript": "^5.7.3",
		"vitest": "^3.0.0"
	},
	"files": [
		"esm",
		"cjs"
	],
	"keywords": [
		"node",
		"nodejs",
		"javascript",
		"js",
		"library",
		"lib",
		"module",
		"typescript",
		"ts",
		"esm",
		"axios",
		"snake",
		"case",
		"snakecase",
		"snake-case",
		"camel",
		"keys",
		"camelcase",
		"camel-case",
		"object"
	]
}
